logoPath = $('.logo-svgPath')
bgFill = $('.logo-bg>img')
logoPath.hover(function(){
    bgFill.css({ 'animation-play-state': 'paused'})
},function(){
    bgFill.css({ 'animation-play-state': ' running'})
})

